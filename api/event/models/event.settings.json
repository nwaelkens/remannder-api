{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "events",
  "info": {
    "name": "Event"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "content": {
      "type": "string",
      "required": true,
      "maxLength": 150,
      "minLength": 5
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "events"
    },
    "date": {
      "type": "date",
      "required": true
    }
  }
}
