{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "moments",
  "info": {
    "name": "Moment"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "content": {
      "type": "string",
      "required": true,
      "maxLength": 255,
      "minLength": 3
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "moments"
    }
  }
}
